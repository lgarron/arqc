{
	"compilerOptions": {
		"strict": true,
		"target": "es2022",
		"module": "es2022",
		"declaration": true,
		"emitDeclarationOnly": true,
		"outDir": "./.temp/types/",
		"skipLibCheck": true,
    "allowJs": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "noFallthroughCasesInSwitch": true,
	},
	"files": ["./src/index.ts"]
}
